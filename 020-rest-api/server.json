{
    "name":"lucee-rest-api",
    "startTimeout":"3600",
    "app":{
        "cfengine":"lucee",
        "restMappings":"/rest/*,/api/*,/*"
    },
    "web":{
        "host":"localhost",
        "servletPassPredicate":"regex( '^/(.+?\\.cf[cm])(/.*)?$' ) or path-prefix-nocase( /rest/ ) or path-prefix-nocase( / )",
        "webroot":"src",
        "http":{
            "port":8888
        }
    },
    "JVM":{
        "javaHome":"D:\\tools\\jdk-21.0.1"
    }
}
