{
    "name":"login-oauth2",
    "description":"Sample App using Lucee and CommandBox",
    "type":"app",
    "version":"1.0.0",
    "dependencies":{
        "jwt-cfml":"^1.2.1",
        "mariadb":"jar:https://repo1.maven.org/maven2/org/mariadb/jdbc/mariadb-java-client/3.5.3/mariadb-java-client-3.5.3.jar",
        "password4j":"jar:https://repo1.maven.org/maven2/com/password4j/password4j/1.8.3/password4j-1.8.3.jar",
        "oauth2-providers":"^2.2.0"
    },
    "scripts":{
        "start":"server start",
        "restart":"server restart",
        "stop":"server stop"
    },
    "installPaths":{
        "jwt-cfml":"src/modules/jwtcfml/",
        "mariadb":"src/lib/mariadb/",
        "password4j":"src/lib/password4j/",
        "oauth2-providers":"src/modules/oauth2providers/"
    }
}
